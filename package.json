{
  "name": "draw-draw-ink",
  "version": "2.1.0",
  "main": "app.js",
  "engines": {
    "node": "10.17.0"
  },
  "scripts": {
    "dev": "nodemon server/index.js",
    "start": "next build && node server/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "test": "jest --watch",
    "test:nowatch": "jest",
    "image:build": "docker build -f ../deployment/draw-draw-ink/Dockerfile -t us-central1-docker.pkg.dev/development-262721/draw-draw-ink-client/ddi-original:\"$(git describe)\" .",
    "image:promote": "docker push us-central1-docker.pkg.dev/development-262721/draw-draw-ink-client/ddi-original:\"$(git describe)\"",
    "image:start": "docker run -p 8080:8080/tcp us-central1-docker.pkg.dev/development-262721/draw-draw-ink-client/ddi-original:\"$(git describe)\"",
    "image:bash": "docker run -p 8080:8080/tcp -it us-central1-docker.pkg.dev/development-262721/draw-draw-ink-client/ddi-original:\"$(git describe)\" /bin/sh",
    "publish": "npm run image:build && npm run image:promote",
    "publish:test": "npm version prerelease --preid=g\"$(git rev-parse --short=5 HEAD)\" && npm run publish",
    "publish:patch": "npm version patch && npm run publish",
    "publish:minor": "npm version minor && npm run publish",
    "publish:major": "npm version major && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deanslamajr/draw-draw-ink.git"
  },
  "author": "Dean Slama Jr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deanslamajr/draw-draw-ink/issues"
  },
  "homepage": "https://github.com/deanslamajr/draw-draw-ink#readme",
  "nodemonConfig": {
    "ignore": [
      "pages/*",
      "scripts"
    ]
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "aws-sdk": "^2.374.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0-beta.3",
    "dom-to-image": "^2.6.0",
    "dotenv": "^6.2.0",
    "emoji-mart": "^2.9.1",
    "express": "^4.16.4",
    "hex-rgb": "^4.1.0",
    "konva": "^2.5.1",
    "lodash": "^4.17.11",
    "newrelic": "^7.5.2",
    "next": "^7.0.2",
    "next-routes": "^1.4.2",
    "pg": "^7.7.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.3.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-ga": "^2.5.7",
    "react-google-recaptcha": "^1.0.5",
    "react-image": "^2.2.0",
    "react-konva": "^16.6.31",
    "react-newline-to-break": "^1.0.6",
    "recaptcha-v3": "^1.5.0",
    "rgb-hex": "^3.0.0",
    "sequelize": "^4.42.0",
    "shortid": "^2.2.14",
    "store2": "^2.7.1",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.9.2",
    "jest": "^24.9.0",
    "nodemon": "^1.18.7",
    "sequelize-cli": "^5.4.0"
  }
}
